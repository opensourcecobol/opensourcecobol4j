#: cobc/cobc.c:472 cobc/parser.y:40
#, c-format
msgid "'%s' not implemented"
msgstr "`%s' は実装されていません"

#: cobc/cobc.c:840
msgid "Usage: cobc [options] file..."
msgstr "使い方: cobc [options] file..."

#: cobc/cobc.c:841
msgid "Options:"
msgstr "オプション:"

#: cobc/cobc.c:842
msgid "  --help                Display this message"
msgstr "  --help                このメッセージを表示します"

#: cobc/cobc.c:843
msgid "  --version, -V         Display compiler version"
msgstr "  --version, -V         コンパイラのバージョンを表示します"

#: cobc/cobc.c:844
msgid "  -v                    Display the commands invoked by the compiler"
msgstr "  -v                    コンパイラが起動したプログラムを表示します"

#: cobc/cobc.c:845
msgid "  -x                    Build an executable program"
msgstr "  -x                    実行可能プログラムをビルドします"

#: cobc/cobc.c:846
msgid "  -m                    Build a dynamically loadable module (default)"
msgstr ""
"  -m                    動的ロード可能モジュールをビルドします (デフォルト)"

#: cobc/cobc.c:847
msgid "  -std=<dialect>        Warnings/features for a specific dialect :"
msgstr "  -std=<方言>           指定した方言に基づいて警告/機能します :"

#: cobc/cobc.c:848
msgid "                          cobol2002   Cobol 2002"
msgstr "                          cobol2002   Cobol 2002"

#: cobc/cobc.c:849
msgid "                          cobol85     Cobol 85"
msgstr "                          cobol85     Cobol 85"

#: cobc/cobc.c:850
msgid "                          ibm         IBM Compatible"
msgstr "                          ibm         IBM互換"

#: cobc/cobc.c:851
msgid "                          mvs         MVS Compatible"
msgstr "                          mvs         MVS互換"

#: cobc/cobc.c:852
msgid "                          bs2000      BS2000 Compatible"
msgstr "                          bs2000      BS2000互換"

#: cobc/cobc.c:853
msgid "                          mf          Micro Focus Compatible"
msgstr "                          mf          Micro Focus互換"

#: cobc/cobc.c:854
msgid "                          default     When not specified"
msgstr "                          default     指定しない"

#: cobc/cobc.c:855
msgid "                        See config/default.conf and config/*.conf"
msgstr ""
"                        config/default.conf および config/*.conf を参照してく"
"ださい"

#: cobc/cobc.c:856
msgid "  -free                 Use free source format"
msgstr "  -free                 自由形式を使用します"

#: cobc/cobc.c:857
msgid "  -free_1col_aster      Use free(1col_aster) source format"
msgstr ""
"  -free_1col_aster      自由形式(かつ第1カラムの*を注釈行の標識とみなす)を使"
"用します"

#: cobc/cobc.c:858
msgid "  -fixed                Use fixed source format (default)"
msgstr "  -fixed                固定形式を使用します (デフォルト)"

#: cobc/cobc.c:859
msgid "  -O, -O2, -Os          Enable optimization"
msgstr "  -O, -O2, -Os          最適化を有功にします"

#: cobc/cobc.c:860
msgid "  -g                    Enable C compiler debug / stack check / trace"
msgstr ""
"  -g                    Cコンパイラのデバッグオプション/スタックチェック/ト"
"レースを有効にします"

#: cobc/cobc.c:861
msgid "  -debug                Enable all run-time error checking"
msgstr "  -debug                すべての実行時エラーチェックを有功にします"

#: cobc/cobc.c:862
msgid "  -o <file>             Place the output into <file>"
msgstr "  -o <ファイル>         出力先を <ファイル> にします"

#: cobc/cobc.c:863
msgid "  -b                    Combine all input files into a single"
msgstr "  -b                    すべての入力ファイルをひとつに結合します"

#: cobc/cobc.c:864
msgid "                        dynamically loadable module"
msgstr "                        動的ロード可能モジュール"

#: cobc/cobc.c:865
msgid "  -E                    Preprocess only; do not compile or link"
msgstr "  -E                    前処理のみ; コンパイルやリンクを行いません"

#: cobc/cobc.c:866
msgid "  -C                    Translation only; convert COBOL to C"
msgstr "  -C                    トランスレートのみ; COBOL から C へ変換します"

#: cobc/cobc.c:867
msgid "  -S                    Compile only; output assembly file"
msgstr "  -S                    コンパイルのみ; アセンブリファイルを出力します"

#: cobc/cobc.c:868
msgid "  -c                    Compile and assemble, but do not link"
msgstr ""
"  -c                    コンパイルとアセンブルを行い、リンクを行いません"

#: cobc/cobc.c:869
msgid ""
"  -t <file>             Generate and place a program listing into <file>"
msgstr "  -t <ファイル>         プログラムリストを <ファイル> に生成します"

#: cobc/cobc.c:870
msgid "  -I <directory>        Add <directory> to copy/include search path"
msgstr ""
"  -I <ディレクトリ>     COPY/INCLUDEの探索パスに <ディレクトリ> を加えます"

#: cobc/cobc.c:871
msgid "  -L <directory>        Add <directory> to library search path"
msgstr ""
"  -L <ディレクトリ>     ライブラリの探索パスに <ディレクトリ> を加えます"

#: cobc/cobc.c:872
msgid "  -l <lib>              Link the library <lib>"
msgstr "  -l <lib>              ライブラリ <lib> をリンクします"

#: cobc/cobc.c:873
msgid "  -B <options>          Add <options> to the C compile phase"
msgstr "  -B <options>          Cコンパイルフェーズに <options> を追加します"

#: cobc/cobc.c:874
msgid "  -Q <options>          Add <options> to the C link phase"
msgstr "  -Q <options>          Cリンクフェーズに <options> を追加します"

#: cobc/cobc.c:875
msgid "  -D <define>           Pass <define> to the C compiler"
msgstr "  -D <define>           Cコンパイラに <define> を渡します"

#: cobc/cobc.c:876
msgid "  -conf=<file>          User defined dialect configuration - See -std="
msgstr "  -conf=<ファイル>      ユーザ定義の方言設定 - -std=を参照してください"

#: cobc/cobc.c:877
msgid "  --list-reserved       Display reserved words"
msgstr "  --list-reserved       予約語の一覧を表示します"

#: cobc/cobc.c:878
msgid "  --list-intrinsics     Display intrinsic functions"
msgstr "  --list-intrinsics     組み込み関数の一覧を表示します"

#: cobc/cobc.c:879
msgid "  --list-mnemonics      Display mnemonic names"
msgstr "  --list-mnemonics      作成者語の一覧を表示します"

#: cobc/cobc.c:880
msgid ""
"  -save-temps(=<dir>)   Save intermediate files (default current directory)"
msgstr ""
"  -save-temps(=<dir>)   中間生成ファイルを保存します (デフォルトはカレント"
"ディレクトリ)"

#: cobc/cobc.c:881
msgid "  -MT <target>          Set target file used in dependency list"
msgstr ""
"  -MT <target>          依存関係リストで使用される対象ファイルを指定します"

#: cobc/cobc.c:882
msgid "  -MF <file>            Place dependency list into <file>"
msgstr "  -MF <ファイル>        依存関係リストを <ファイル> に生成します"

#: cobc/cobc.c:883
msgid "  -ext <extension>      Add default file extension"
msgstr "  -ext <extension>      既定のファイル拡張子を追加します"

#: cobc/cobc.c:884
msgid "  -assign_external      Set the file assign to external"
msgstr ""
"  -assign_external      すべてのASSIGN句に省略値EXTERNALが指定されたとみなし"
"ます"

#: cobc/cobc.c:885
msgid "  -reference_check      Set reference check in runtime"
msgstr "  -reference_check      実行時の参照チェックを有効にします"

#: cobc/cobc.c:886
msgid ""
"  -constant(=<name\"value\">) define <name> to <value> for $IF statement"
msgstr ""
"  -constant(=<name\"value\">) $IF 文で評価する定数名 <name> に 値 <value> を"
"設定します"

#: cobc/cobc.c:890
msgid "  -W                    Enable ALL warnings"
msgstr "  -W                    すべての警告を有功にする"

#: cobc/cobc.c:891
msgid "  -Wall                 Enable all warnings except as noted below"
msgstr "  -Wall                 以下を除くすべての警告を有功にする"

#: cobc/cobc.c:896
msgid " (NOT set with -Wall)"
msgstr " (-Wall指定時は適用されません)"

#: cobc/cobc.c:1092
#, c-format
msgid "Invalid option -std=%s\n"
msgstr "不当なオプションです -std=%s\n"

#: cobc/cobc.c:1685
msgid "-MT must be given to specify target file\n"
msgstr "ターゲットファイルを -MT で指定して下さい\n"

