AT_SETUP([Version strings in generated Java files])

AT_DATA([prog.cbl], [
       IDENTIFICATION              DIVISION.
       PROGRAM-ID. prog.
       DATA                        DIVISION.
       WORKING-STORAGE             SECTION.
       01  WK-X         PIC X(02).
       PROCEDURE                   DIVISION.
       MAIN-RTN.
])

AT_CHECK([${COBJ} -C prog.cbl])
AT_CHECK([head -n 1 prog.java  | grep -e '/\* Generated by opensource COBOL 4J .* \*/' > /dev/null])

AT_CLEANUP
